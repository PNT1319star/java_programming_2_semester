<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.csjchoisoojong.controllers.AskWindowController"
            prefHeight="520.0" prefWidth="320.0">
    <GridPane hgap="10.0" layoutX="60.0" layoutY="250.0" prefHeight="520.0" prefWidth="320.0" vgap="10.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="nameLabel" text="Name" GridPane.halignment="CENTER">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="xLabel" layoutX="64.0" layoutY="32.0" text="X" GridPane.halignment="CENTER" GridPane.rowIndex="1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="yLabel" layoutX="64.0" layoutY="32.0" text="Y" GridPane.halignment="CENTER" GridPane.rowIndex="2">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="annualTurnoverLabel" layoutX="90.0" layoutY="160.0" text="Annual Turnover" GridPane.halignment="CENTER" GridPane.rowIndex="3">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="fullNameLabel" layoutX="90.0" layoutY="160.0" text="Full Name" GridPane.halignment="CENTER" GridPane.rowIndex="4">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="employeesCountLabel" layoutX="90.0" layoutY="160.0" text="Employees Count" GridPane.halignment="CENTER" GridPane.rowIndex="5">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="typeLabel" layoutX="64.0" layoutY="32.0" text="Type" GridPane.halignment="CENTER" GridPane.rowIndex="6">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="addressLabel" layoutX="90.0" layoutY="160.0" text="Address" GridPane.halignment="CENTER" GridPane.rowIndex="7">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="nameField" GridPane.columnIndex="1"/>
        <TextField fx:id="xField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="yField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="annualTurnoverField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <TextField fx:id="fullNameField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <TextField fx:id="employeesCountField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <ComboBox fx:id="typeComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <TextField fx:id="addressField" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
        <Button fx:id="enterButton" mnemonicParsing="false" onAction="#enterButtonOnAction" prefWidth="130.0"
                text="Enter" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="8"/>
    </GridPane>
</AnchorPane>
